{
	"name": "<%= repoName %>",
	"version": "0.0.0",
	"description": "<%= repoDescription %>",
	"license": "BSD",
	"repository": "<%= repoOwner %>/<%= repoName %>",
	"engines": {
		"node": ">=0.12.0",
		"npm": ">=3.0.0"
	},<% if (isNodeModule) { %>
	"main": "lib/<%= capitalizeName %>.js",
	"browser": "src/<%= capitalizeName %>.js",<% } else { %>
	"main": "src/<%= capitalizeName %>.js",<% } %>
	"files": [<% if (isNodeModule) { %>
		"lib",<% } %>
		"src",
		"test"
	],
	"scripts": {<% if (isNodeModule) { %>
		"compile": "babel --presets es2015 -d lib/ src/",
		"publish": "npm run compile",<% } %>
		"test": "gulp test"
	},
	"keywords": [
		"metal"
	],
	"dependencies": {
    "bootstrap": "^3.3.6"<% if (superClass === 'Attribute') { %>,
		"metal-attribute": "metal/metal-attribute#master"<% } else if (superClass === 'Component') { %>,
		"metal-component": "metal/metal-component#master",
		"metal-soy": "metal/metal-soy#master"<% } %>
	},
	"devDependencies": {<% if (isNodeModule) { %>
		"babel-cli": "^6.4.5",
		"babel-preset-es2015": "^6.3.13",<% } %><% if (!defaultKarmaConfig) { %>
		"babel-preset-metal": "^3.0.0",
		"chai": "^2.3.0",<% } %>
		"gulp": "^3.8.11",
		"gulp-metal": "^1.0.0"<% if (!defaultKarmaConfig) { %>,
		"isparta": "^4.0.0"<% } %><% if (buildFormat === 'jquery') { %>,
		"jquery": "^2.2.0"<% } %><% if (!defaultKarmaConfig) { %>,
		"karma": "^0.13.9",
		"karma-babel-preprocessor": "^6.0.1",
		"karma-chai": "^0.1.0",
		"karma-chrome-launcher": "^0.2.0",
		"karma-commonjs": "0.0.13",
		"karma-coverage": "^0.5.1",
		"karma-mocha": "^0.2.0",
		"karma-source-map-support": "1.0.0",
		"mocha": "^2.2.5"<% } %><% if (buildFormat === 'amd') { %>,
		"requirejs": "^2.1.0"<% } %>
	}
}
